    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "gen:models": "npx @openapitools/openapi-generator-cli generate -i swagger-spec.json -g typescript -o ./libs/core/src/lib/api/ --skip-validate-spec",
    "gen:swagger": "openapi-generator-cli",
    "dev:build": "docker-compose build",
    "start:db": "docker-compose up mediapostgres db",
    "start:services": "docker-compose up",
    "stop:db": "docker-compose down mediapostgres db",
    "stop:services": "docker-compose down",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "start": "nx serve --watch",
    "start:auth": "nest start --path apps/media-auth/tsconfig.json --watch",
    "start:ts": "ts-node -r tsconfig-paths/register --project tsconfig.base.json src/main.ts",
    "seed:users": "ts-node --project scripts/tsconfig.json -r  tsconfig-paths/register scripts/gen-users.script.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "debug": "nodemon -L --config nodemon-docker-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "build:prod": "rimraf dist && npm run build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
