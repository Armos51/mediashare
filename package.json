    "nx": "nx",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "init:dev": "docker-compose -p mediashare up --build -V -d",
    "start:dev": "docker-compose -p mediashare up -V",
    "stop:dev": "docker-compose -p mediashare down",
    "build": "nest build",
    "affected:libs": "nx affected:libs",
