{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.0.1",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/apps/mediashare/src/api",
        "glob": "swagger-spec.{json,yaml}",
        "additionalProperties": {
          "supportsES6": "true",
          "modelPropertyNaming": "original",
          "useSingleRequestParameter": true,
          "withSeparateModelsAndApi": true,
          "apiPackage": "services",
          "modelPackage": "models"
        }
      },
      "v3.0": {
        "generatorName": "typescript-rxjs",
        "output": "#{cwd}/apps/mediashare/src/rxjs-api",
        "glob": "swagger-spec.{json,yaml}",
        "additionalProperties": {
          "supportsES6": "true",
          "modelPropertyNaming": "original",
          "useSingleRequestParameter": true,
          "withInterfaces": true,
          "withSeparateModelsAndApi": true
        }
      }
    }
  }
}
